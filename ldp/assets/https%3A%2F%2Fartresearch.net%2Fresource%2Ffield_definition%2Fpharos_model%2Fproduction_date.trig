    <http://spinrdf.org/sp#text> """PREFIX crm: <http://www.cidoc-crm.org/cidoc-crm/>

SELECT ?value ?provUri WHERE {
  $subject crm:P108i_was_produced_by/crm:P9_consists_of* ?production .
  ?production crm:P4_has_time-span ?timeSpan.
  
  
  BIND(IRI(CONCAT(\"http://artresearch.net/resource/e13/\", SHA256(CONCAT(STR(?production), STR(crm:P4_has_time-span), STR(?timeSpan))))) AS ?attribution)
  
  ?attribution crm:P134_continued* ?provUri .
  ?provUri crm:P9i_forms_part_of ?cataloging .
  ?cataloging crm:P2_has_type <http://vocab.getty.edu/aat/300054628> .

  
  OPTIONAL {
    ?timeSpan crm:P190_has_symbolic_content ?staticValue .
  }
  OPTIONAL {
    ?timeSpan crm:P82a_begin_of_the_begin ?begin .
    ?timeSpan crm:P82b_end_of_the_end ?end .
    BIND(CONCAT(?begin, \"#\", ?end) AS ?range ) .
  }
  OPTIONAL {
    ?timeSpan crm:P82_at_some_time_within ?within .
  }
  FILTER(BOUND(?staticValue) || BOUND(?within) || BOUND(?range)) .
  BIND(COALESCE(?staticValue, ?within, ?range) AS ?value) .
  
  
}""" .
  
